function getRandomQuote() \{\par
const randomIndex = Math.floor(Math.random() * quotes.length);\par
return quotes[randomIndex];\par
\}\par
function BlogList() \{\par
const [selectedPost, setSelectedPost] = useState(null);\par
const handlePostClick = (index) => \{\par
setSelectedPost(blogPosts[index]);\par
\};\par
return (\par
<div>\par
<h1>Blog Posts</h1>\par
<ul>\par
\{blogPosts.map((post, index) => (\par
<li key=\{index\} onClick=\{() => handlePostClick(index)\}>\par
\{post.title\}\par
</li>\par
))\}\par
</ul>\par
\{selectedPost && (\par
<BlogPost\par
title=\{selectedPost.title\}\par
content=\{`$\{getRandomQuote()\} $\{selectedPost.content\}`\}\par
author=\{selectedPost.author\}\par
/>\par
)\}\par
</div>\par
);\par
\}\par
export default BlogList;\par
