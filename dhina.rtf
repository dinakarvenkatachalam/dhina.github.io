PostTitle.js\par
import React from 'react';\par
function PostTitle(\{ title \}) \{\par
return <h2>\{title\}</h2>;\par
\}\par
export default PostTitle;\par
PostContent.js\par
import React from 'react';\par
function PostContent(\{ content \}) \{\par
return <p>\{content\}</p>;\par
\}\par
export default PostContent;\par
PostAuthor.js\par
import React from 'react';\par
function PostAuthor(\{ author \}) \{\par
return <p>Author: \{author\}</p>; \}\par
export default PostAuthor;\par
BlogPost.js\par
import React from 'react';\par
import PostTitle from './PostTitle';\par
import PostContent from './PostContent';\par
import PostAuthor from './PostAuthor';\par
function BlogPost(\{ title, content, author \}) \{\par
return (\par
<div className="blog-post">\par
<PostTitle title=\{title\} />\par
<PostContent content=\{content\} />\par
<PostAuthor author=\{author\} />\par
</div>\par
);\par
\}\par
export default BlogPost;\par
